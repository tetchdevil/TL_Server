<?xml version="1.0" encoding="utf-8"?>
<Terrain xsize="64" zsize="64" tileSize="32">

    <scale x="100" y="100" z="100" />

    <materials>
        <template name="OneLayer" material="Terrain/OneLayer" />
        <template name="OneLayerLightmap" material="Terrain/OneLayerLightmap" />
        <template name="TwoLayer" material="Terrain/TwoLayer" />
        <template name="TwoLayerLightmap" material="Terrain/TwoLayerLightmap" />
        <fog_replacement
                none="Terrain/OneLayer_ps"
                exp="Terrain/OneLayer_ps%fog_exp"
                exp2="Terrain/OneLayer_ps%fog_exp2"
                linear="Terrain/OneLayer_ps%fog_linear" />
        <fog_replacement
                none="Terrain/TwoLayer_ps"
                exp="Terrain/TwoLayer_ps%fog_exp"
                exp2="Terrain/TwoLayer_ps%fog_exp2"
                linear="Terrain/TwoLayer_ps%fog_linear" />
        <fog_replacement
                none="Terrain/OneLayerLightmap_ps"
                exp="Terrain/OneLayerLightmap_ps%fog_exp"
                exp2="Terrain/OneLayerLightmap_ps%fog_exp2"
                linear="Terrain/OneLayerLightmap_ps%fog_linear" />
        <fog_replacement
                none="Terrain/TwoLayerLightmap_ps"
                exp="Terrain/TwoLayerLightmap_ps%fog_exp"
                exp2="Terrain/TwoLayerLightmap_ps%fog_exp2"
                linear="Terrain/TwoLayerLightmap_ps%fog_linear" />
    </materials>
</Terrain>
